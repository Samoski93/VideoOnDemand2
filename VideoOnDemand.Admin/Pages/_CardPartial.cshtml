@model VideoOnDemand.Admin.Models.CardViewModel

<a href="@Model.Url" class="card" style="@Model.BackgroundColor">
    <div class="class-content">
        <div class="card-data">
            <h3>@Model.Count</h3>
            <p>@Model.Description</p>
        </div>
        <span class="glyphicon glyphicon-@Model.Icon.ToLower()"></span>
    </div>
</a>